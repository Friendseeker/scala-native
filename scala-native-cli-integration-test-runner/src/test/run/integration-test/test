> compile
# -- Link and check if outpath exists
> runTest --main Main$ --outpath target/out1 --disable-debug
$ exists target/out1
# -- Fail to link without all required options
> runTest --outpath target/out2 --disable-debug
-$ exists target/out2
# -- Fail to link with an incorrect option
> runTest --main Main$ --outpath target/out3 --disable-debug --gc fast
-$ exists target/out3
# -- Fail to link with an unspecified option
> runTest --main Main$ --outpath target/out4 --disable-debug --unspecified
-$ exists target/out4
# -- Do not write nir files if not specified
> runTest --main Main$ --outpath target/out5 --disable-debug
-$ exists optimized.hnir
$ exists target/out5
# -- Write nir files to cwd if specified
$ mkdir native-dir
> runTest --main Main$ --outpath target/out6 --disable-debug --workdir native-dir --dump
$ exists native-dir/optimized.hnir
$ exists target/out6
# -- Fail on an incorrect link-time property
> runTest --main Main$ --outpath target/out7 --disable-debug --ltp key=ok
-$ exists target/out7
